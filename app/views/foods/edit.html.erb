<%= form_for @food do |f| %> 
    <%= f.label 'Name' %>
    <%= f.text_field :name %><br></br>

    <%= f.label 'Calories' %>
    <%= f.number_field :calories %><br></br>

    <%= f.label 'Protein' %>
    <%= f.number_field :protein %><br></br>

    <%= f.label 'Carbohydrates' %>
    <%= f.number_field :carbohydrates %><br></br>

    <%= f.label 'Fats' %>
    <%= f.number_field :fats %><br></br>

    <%= f.submit "Submit" %>
    <% end %>