<%= form_for @food do |f| %>
    <%= f.fields_for :foods do |food| %>
        <%= food.label :name %>
        <%= food.text_field :name %><br>
    
        <%= food.label :calories %>
        <%= food.number_field :calories %><br>

        <%= food.label :protein %>
        <%= food.number_field :protien %><br>

        <%= food.label :carbohyrdates%>
        <%= food.number_field :carbohydrates %><br>

        <%= food.label :fats%>
        <%= food.number_field :fats %><br>

    <% end %>
    <%= f.submit "Sumbit" %>
<% end %>